<%= link_to "RedditDemo", root_path, :class => 'brand' %>
<ul class="nav" align="right">
    <li>
    <%= link_to('Users', users_path) %>
    </li>
    <% if current_user %>
      <li>
        <%= link_to "Logged in as " + current_user.email, users_path %>.
      </li>
      <li>
      <%= link_to "Log out", log_out_path %>
      </li>
    <% else %>
      <li>
      <%= link_to "Sign up", sign_up_path %>
      </li>
      <li>
      <%= link_to "Login", log_in_path %>
      </li>
    <% end %>
</ul>